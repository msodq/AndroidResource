<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:fillAfter="true">

    <translate
        android:duration="600"
        android:fillAfter="true"
        android:fromYDelta="100%p"
        android:toYDelta="0%p" />
    <rotate
        android:duration="600"
        android:fromDegrees="10"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toDegrees="0" />

</set>